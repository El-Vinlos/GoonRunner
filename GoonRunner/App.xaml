<Application x:Class="GoonRunner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             StartupUri="MVVM/View/LogIn.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ControlsDictionary/>
                <ResourceDictionary Source="pack://application:,,,/Theme/TextboxTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/ButtonTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/CLoseButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/MinimizeButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/Groupbox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/MenuButtonTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/NavButtonTheme.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Theme/GridSplitterTheme.xaml"/>
             </ResourceDictionary.MergedDictionaries>
         </ResourceDictionary>
    </Application.Resources>
</Application>
